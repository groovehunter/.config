general {
        output_format = "dzen2"
        colors = true
        interval = 5
}

order += "time"
order += "ethernet br0"
order += "disk /"
order += "load"


ethernet eth0 {
        # if you use %speed, i3status requires the cap_net_admin capability
        format_up = "E: %ip (%speed)"
        format_down = "E: down"
}

time {
    format = "%H:%M:%S"
}

load {
        format = "%5min"
}

disk "/" {
        format = "%free"
}

